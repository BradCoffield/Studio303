<ul>
  {% for article in articlelist %}
    <li>
      <strong>
        <a href="{{ article.url | url }}">{{ article.data.title }}</a>
      </strong>
      -
      <time datetime="{{ article.date | htmlDateString }}">{{ article.date | readableDate }}</time>
      -
      <span>{{ article.data.description }}</span>
    </li>
  {% endfor %}
</ul>
